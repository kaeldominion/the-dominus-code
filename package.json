    "db:migrate:create": "./scripts/create-migration.sh",
    "db:migrate:reset": "prisma migrate reset",
    "db:seed": "prisma db seed",
    "db:seed:admin": "tsx scripts/seed-admin.ts",
    "db:migrate:neon": "node scripts/migrate-neon.js",
    "db:migrate:prod": "./scripts/migrate-production.sh",
    "db:setup:prod": "./scripts/setup-production-env.sh",
    "postinstall": "npx prisma generate",
    "test:load:local": "k6 run k6-load-test.js",
    "test:load:prod": "BASE_URL=https://thedominuscode.com k6 run k6-load-test.js",
    "test:load:quick": "k6 run --duration 30s --vus 10 k6-load-test.js"
  },
