  "scripts": {
    "dev": "next dev -p 3003",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:push": "npx dotenv-cli -e .env.local -- prisma db push",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:migrate": "npx dotenv-cli -e .env.local -- prisma migrate dev",
